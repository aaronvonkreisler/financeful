{
  "name": "financeful-server",
  "version": "1.0.0",
  "description": "GraphQL server for financeful",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "build": "tsc",
    "migrate": "yarn prisma migrate dev --preview-feature",
    "migrate:test": "DATABASE_URL=postgresql://prisma:prisma@localhost:5433/tests yarn prisma migrate dev --name init",
    "studio": "yarn prisma studio",
    "seed:db": "yarn prisma db seed --preview-feature",
    "reset:db": "yarn prisma migrate reset --skip-seed",
    "docker:testdb": "docker compose -f docker-compose.test.yml up -d",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "test:integration": "yarn docker:testdb && yarn migrate:test && jest -i",
    "test:integration:watch": "yarn docker:testdb && yarn migrate:test && yarn test:watch",
    "kill:tests": "docker compose -f docker-compose.test.yml down"
  },
  "keywords": [],
  "author": "Aaron von Kreisler",
  "license": "ISC",
  "_moduleAliases": {
    "@Modules": "dist/modules",
    "@Lib": "dist/lib",
    "@Shared": "dist/shared"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.34",
    "@types/prompt": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chalk": "^4.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-node": "^11.1.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.14",
    "mockdate": "^3.0.5",
    "prettier": "^2.2.1",
    "prisma": "^2.22.1",
    "prompt": "^1.1.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "apollo-datasource": "^0.7.3",
    "apollo-server-express": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nanoid": "^3.1.22",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
